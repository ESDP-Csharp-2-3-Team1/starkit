@model List<Category>

@if (Model.Count != 0)
{
    @foreach (var category in Model)
    {
        <tr>
            <td>@category.Id</td>
            <td>@category.Name</td>
            <td>
                <button type="button" class="btn btn-danger" onclick="openModal('@category.Id')">Удалить</button>
                <a href="#">Редактировать</a>
            </td>
        </tr>
    }   
}