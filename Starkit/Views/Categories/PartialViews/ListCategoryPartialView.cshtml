@model List<Category>

@if (Model.Count != 0)
{
    @foreach (var category in Model)
    {
        <tr>
            <td>@category.Name</td>
            <td>@category.CreateTime</td>
            <td>@category.EditedTime</td>
            <td>
                <button type="button" class="btn btn-danger" onclick="openModal('@category.Id')">Удалить</button>
                <a class="btn btn-primary" asp-action="Edit" asp-route-id="@category.Id">Редактировать</a>
            </td>
        </tr>
    }   
}