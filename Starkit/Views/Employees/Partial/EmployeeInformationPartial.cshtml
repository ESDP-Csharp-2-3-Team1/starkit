@model SuperAdminIndexViewModel


<div class="form-group  ed2-form-group">
    <div class="ed2-form-label">
        <label asp-for="User.Name">Имя:</label>
        <span title="Обязательное поле" class="form-required">*</span>
    </div>
    <div class="ed2-form-input">
        <div class="boxx2">
            <input type="text" class="form-control" asp-for="User.Name">
            <span asp-validation-for="User.Name"></span>
        </div>
    </div>
</div>
<div class="form-group  ed2-form-group">
    <div class="ed2-form-label">
        <label asp-for="User.SurName">Фамилия:</label>
        <span title="Обязательное поле" class="form-required">*</span>
    </div>
    <div class="ed2-form-input">
        <div class="boxx2">
            <input type="text" class="form-control" asp-for="User.SurName">
            <span asp-validation-for="User.SurName"></span>
        </div>
    </div>
</div>
<div class="form-group  ed2-form-group">
    <div class="ed2-form-label">
        <label asp-for="User.Email">Email:</label>
        <span title="Обязательное поле" class="form-required">*</span>
    </div>
    <div class="ed2-form-input">
        <div class="boxx2">
            <input class="form-control" asp-for="User.Email">
            <span asp-validation-for="User.Email"></span>
        </div>
    </div>
</div>
