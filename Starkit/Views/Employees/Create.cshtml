@model EmployeeViewModel

@{
    ViewBag.Title = "Регистрация нового сотрудника ";
    Layout = "_Layout";
}
<style>
    #dropdown2{
        display: block;
    }
    #dropdown2-li-3{
          margin-left: 15px;
            background: #9ad9ea;
    }
    #dropdown2-li-3 a{
                color: #262626;
                font-weight: 600;
                border-left: 2px solid #41728b;
    }
    #dropdown2-li-3 a:hover{
        color: #262626;
    }
 </style>

<div class="employee-create-container">
    <h2>Регистрация нового сотрудника </h2>
    <div asp-validation-summary="ModelOnly" style="text-align: center"></div>
    <form asp-action="Create" asp-controller="Employees" method="post">
        @{
            await Html.RenderPartialAsync("Partial/EmployeeRegisterFormPartial");
        }
    </form>
</div>
@section Scripts
{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    
}
