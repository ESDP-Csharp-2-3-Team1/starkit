@model User

@{
    ViewBag.Title = "Сайт визитка";
    Layout = "_LayoutSiteCard";
}

<div class="container-fluid">
    @{
        await Html.RenderPartialAsync("Partial/BlockCarouselPartial");
        await Html.RenderPartialAsync("Partial/BlockOrderPartial");
        await Html.RenderPartialAsync("Partial/BlockSpecialDishPartial");
    }
</div>

@section Scripts
{
    <script type="text/javascript">
        $(document).on('ready', function() {
             $(".center").slick({
               dots: true,
               infinite: true,
               centerMode: true,
               slidesToShow: 4,
               slidesToScroll: 3
             });
           });
    </script>
}
