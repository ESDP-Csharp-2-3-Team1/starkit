    </main>
<footer class="border-top footer text-muted">
    @{
        await Html.RenderPartialAsync("Partial/_PartialFooterSiteCard");
    }
    <div style="padding-top: 19px; background-color: #e7272d;">
        <div class="container" style="line-height: normal">
            <p style="padding-bottom: 20px; color: #ffffff;font-size: 15px;text-align: center">&copy; Copyright Starkit 2020. Все Права Защищены
                <a asp-area="" style="color: #ffffff;">StarKit</a></p>
        </div>
    </div>
