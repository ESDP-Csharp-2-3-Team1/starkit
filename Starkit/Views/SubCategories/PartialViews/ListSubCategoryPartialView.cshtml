@model List<SubCategory>

@if (Model.Count != 0)
{
    @foreach (var subCategories in Model)
    {
        <tr>
            <td>@subCategories.Name</td>
            <td>@subCategories.Category.Name</td>
            <td>@subCategories.CreateTime</td>
            <td>@subCategories.EditedTime</td>
            <td>
                <button type="button" class="btn btn-danger" onclick="openModal('@subCategories.Id')">Удалить</button>
                <a class="btn btn-primary" asp-action="Edit" asp-route-id="@subCategories.Id">Редактировать</a>
            </td>
        </tr>
    }   
}