@model List<Order>

@if (Model.Count != 0)
{
    @foreach (var order in Model)
    {
        <tr>
            <td class="table-orders-index">@order.OrderNum</td>
            <td class="table-orders-index">@order.ContactNumber</td>
            <td class="table-orders-index">@order.Name</td>
            <td class="table-orders-index">@order.Address</td>
            <td class="table-orders-index">@order.OrderTime</td>
            <td class="table-orders-index">@order.Status</td>
            <td class="table-orders-index">
                <img class="icons-orders-index" src="https://img.icons8.com/ios/24/000000/zoom-in.png"/>
                <img onclick="deleteOrder('@order.Id')" class="icons-orders-index" src="https://img.icons8.com/wired/24/000000/delete-forever.png"/>
            </td>
        </tr>   
    }
}
