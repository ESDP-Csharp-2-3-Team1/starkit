@model List<Order>

@if (Model.Count != 0)
{
    @foreach (var order in Model)
    {
        <tr onclick="order.Id" style="cursor: pointer">
            <td class="table-orders-index">@order.OrderNum</td>
            <td class="table-orders-index">@order.Name</td>
            <td nowrap="nowrap" class="table-orders-index">@order.OrderTime</td>
            <td nowrap="nowrap" class="table-orders-index">@order.Address</td>
            @if (order.Status == Status.Доставке)
            {
                <td class="table-orders-index">В доставке</td>   
            }
            else
            {
                <td class="table-orders-index">@order.Status</td>   
            }
        </tr>   
    }
}
