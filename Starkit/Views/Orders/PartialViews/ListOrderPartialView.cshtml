@model List<Order>

@if (Model.Count != 0)
{
    @foreach (var order in Model)
    {
        <tr>
            <td class="table-orders-index">@order.OrderNum</td>
            <td class="table-orders-index">@order.ContactNumber</td>
            <td class="table-orders-index">@order.Name</td>
            <td class="table-orders-index">@order.Address</td>
            <td class="table-orders-index">@order.OrderTime</td>
            <td class="table-orders-index">@order.Status</td>
        </tr>   
    }
}
