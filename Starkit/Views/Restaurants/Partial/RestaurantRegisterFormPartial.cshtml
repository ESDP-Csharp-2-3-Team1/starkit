@model Restaurant

<div class="form-group  ed2-form-group">
    <div class="ed2-form-label">
        <label asp-for="NameRestaurant">Название завидения</label>
    </div>
    <div class="ed2-form-input">
        <div class="boxx2">
            <input type="text" class="form-control" asp-for="NameRestaurant">
            <span asp-validation-for="NameRestaurant"></span>
        </div>
    </div>
</div>
<div class="form-group  ed2-form-group">
    <div class="ed2-form-label">
        <label asp-for="PhoneNumber">Телефон</label>
    </div>
    <div class="ed2-form-input">
        <div class="boxx2">
            <input type="text" class="form-control" asp-for="PhoneNumber">
            <span asp-validation-for="PhoneNumber"></span>
        </div>
    </div>
</div>
<div class="form-group  ed2-form-group">
    <div class="ed2-form-label">
        <label asp-for="ContactPerson">Контактное лицо</label>
    </div>
    <div class="ed2-form-input">
        <div class="boxx2">
            <input type="text" class="form-control" asp-for="ContactPerson">
            <span asp-validation-for="ContactPerson"></span>
        </div>
    </div>
</div>
<div class="form-group  ed2-form-group">
    <div class="ed2-form-label">
        <label asp-for="Address">Адрес</label>
    </div>
    <div class="ed2-form-input">
        <div class="boxx2">
            <input type="text" class="form-control" asp-for="Address">
            <span asp-validation-for="Address"></span>
        </div>
    </div>
</div>
<div class="form-group  ed2-form-group">
    <div class="ed2-form-label">
        <label asp-for="RestaurantInformation">О заведении</label>
    </div>
    <div class="ed2-form-input">
        <div class="boxx2">
            <input type="text" class="form-control" asp-for="RestaurantInformation">
            <span asp-validation-for="RestaurantInformation"></span>
        </div>
    </div>
</div>
<div class="form-group  ed2-form-group">
    <div class="ed2-form-label">
        <label asp-for="DomainAvailability">У меня уже есть домен</label>
    </div>
    <div class="ed2-form-input">
        <div class="boxx2">
            <div class="form-check form-check-inline">
              <input asp-for="DomainAvailability" class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1">
              <label  class="form-check-label" for="inlineRadio1">Да</label>
            </div>
            <div class="form-check form-check-inline">
              <input asp-for="DomainAvailability" class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2">
              <label class="form-check-label" for="inlineRadio2">Нет</label>
            </div>
        </div>
    </div>
</div>